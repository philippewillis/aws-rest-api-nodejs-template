{
  "name": "aws-rest-api-template",
  "version": "0.0.1",
  "description": "Simple AWS Lambda to be added to a AWS API Gateway application",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "node build && node dist/index.js",
    "dev": "esbuild src/index.ts --bundle --sourcemap --platform=node --target=es2020 --outfile=build/index.js --watch",
    "test": "NODE_ENV=test jest --verbose --coverage --watchAll",
    "test:ci": "NODE_ENV=test jest --verbose",
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=build/index.js"
  },
  "keywords": [
    "AWS",
    "Lambda",
    "API Gateway"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@jest/types": "^29.6.3",
    "@types/aws-lambda": "^8.10.137",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lambda-router": "^2.12.0"
  }
}
